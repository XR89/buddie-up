<div class="container text-left">
  <h1>All users</h1>
  <ul>
  <% @users.each do |user| %>
    <li>
      <% if user.photo.attached? %>
                <%= cl_image_tag user.photo.key, class: "avatar mt-2" %>
            <% else %>
                <%= image_tag "default-profile.jpeg", class: "avatar mt-2" %>
            <% end %>
      <%= link_to user.username, user_path(user) %>
    </li>
  <% end %>
  </ul>
</div>
